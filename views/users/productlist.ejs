                                        <div class="widget">


                                            <h3 class="widget-title">Categories</h3><!-- End .widget-title -->
                                            <hr>

                                            <div class="widget-body">
                                                <div class="filter-items">





                                                    <div class="filter-group">
                                                        <% category.forEach((cat)=> {
                                                            const isSelected =
                                                            selectedCategories.includes(cat.category);
                                                            %>
                                                            <div>
                                                                <input name="category" type="checkbox"
                                                                    value="<%= cat.category %>" id="product-cat"
                                                                    <%=isSelected ? 'checked' : '' %>>
                                                                <label for="product-cat">
                                                                    <%= cat.category %>
                                                                </label>
                                                            </div>
                                                            <% }); %>
                                                    </div>
                                                </div><!-- End .widget -->
                                            </div><!-- End .widget -->
                                        </div><!-- End .widget -->


                                        <div class="widget">
                                            <h3 class="widget-title">Price</h3><!-- End .widget-title -->

                                            <div class="widget-body">

                                                <div class="filter-group">
                                                    <label>Price Range:</label>
                                                    <div>
                                                        <input type="radio" id="priceAll" name="price" value="all"
                                                            <%=selectedPriceRange==='all' ? 'checked' : '' %>>
                                                        <label for="priceAll">All Prices</label>
                                                    </div>
                                                    <div>
                                                        <input type="radio" id="priceUnder25" name="price"
                                                            value="under25" <%=selectedPriceRange==='under25'
                                                            ? 'checked' : '' %>>
                                                        <label for="priceUnder25">Under ₹20,000</label>
                                                    </div>
                                                    <div>
                                                        <input type="radio" id="price25to50" name="price" value="25to50"
                                                            <%=selectedPriceRange==='25to50' ? 'checked' : '' %>>
                                                        <label for="price25to50"> ₹20,000 to ₹40,000</label>
                                                    </div>
                                                    <div>
                                                        <input type="radio" id="price50to100" name="price"
                                                            value="50to100" <%=selectedPriceRange==='50to100'
                                                            ? 'checked' : '' %>>
                                                        <label for="price50to100"> ₹40,000 to ₹60,000</label>
                                                    </div>
                                                    <div>
                                                        <input type="radio" id="price100to200" name="price"
                                                            value="100to200" <%=selectedPriceRange==='100to200'
                                                            ? 'checked' : '' %>>
                                                        <label for="price100to200"> ₹60,000 to ₹80,000</label>
                                                    </div>
                                                    <div>
                                                        <input type="radio" id="price200above" name="price"
                                                            value="200above" <%=selectedPriceRange==='200above'
                                                            ? 'checked' : '' %>>
                                                        <label for="price200above"> ₹80,000 & Above</label>
                                                    </div>
                                                </div>

                                            </div><!-- End .widget-body -->
                                        </div><!-- End .widget -->



                                        <div class="widget">
                                            <h3 class="widget-title">Colour</h3><!-- End .widget-title -->
                                            <div class="widget-body">
                                                <div class="filter-group">
                                                    <label>Color:</label>
                                                    <div>
                                                        <input type="checkbox" id="colorBlack" name="color"
                                                            value="Black" <%=selectedColors.includes('Black')
                                                            ? 'checked' : '' %>>
                                                        <label for="colorBlack">Black</label>
                                                    </div>
                                                    <div>
                                                        <input type="checkbox" id="colorWhite" name="color"
                                                            value="White" <%=selectedColors.includes('White')
                                                            ? 'checked' : '' %>>
                                                        <label for="colorWhite">White</label>
                                                    </div>
                                                    <div>
                                                        <input type="checkbox" id="colorBlue" name="color" value="Blue"
                                                            <%=selectedColors.includes('Blue') ? 'checked' : '' %>>
                                                        <label for="colorBlue">Blue</label>
                                                    </div>
                                                    <div>
                                                        <input type="checkbox" id="colorPink" name="color" value="Pink"
                                                            <%=selectedColors.includes('Pink') ? 'checked' : '' %>>
                                                        <label for="colorPink">Pink</label>
                                                    </div>
                                                    <!-- Add more color checkboxes here -->
                                                </div>
                                            </div><!-- End .widget-body -->
                                        </div><!-- End .widget -->


                                        <button type="submit" class="btn btn-outline-primary-2 ">Apply Filters <i
                                                class="fa-solid fa-arrow-right"></i> </button> <br>
                                        <a href="/productlist" class="btn btn-primary mt-3 "> Reset all <i
                                                class="fa-solid fa-xmark"></i> </a>


                                        <!-- End .sidebar sidebar-shop -->


                                </aside><!-- End .col-lg-3 -->

                            </div><!-- End .row -->
                        </div><!-- End .container -->
                    </div><!-- End .page-content -->
                </form>

