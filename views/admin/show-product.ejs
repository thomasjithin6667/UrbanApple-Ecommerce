<head>
    <meta charset="utf-8">
    <title>Admin Dashboard</title>
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta property="og:title" content="">
    <meta property="og:type" content="">
    <meta property="og:url" content="">
    <meta property="og:image" content="">
    <!-- Favicon -->
    <link rel="shortcut icon" href="assets/images/icons/apple-touch-icon.png" type="image/x-icon">
  <link rel="apple-touch-icon" sizes="180x180" href="assets/images/icons/apple-touch-icon.png">
  <link rel="icon" type="image/png" sizes="32x32" href="assets/images/icons/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="16x16" href="assets/images/icons/favicon-16x16.png">
    <!-- Template CSS -->
    <link href="assetsB/css/main.css" rel="stylesheet" type="text/css" />
</head>

<body>
    <div class="screen-overlay"></div>
    <%- include('../partials/ecomAdminAside') %>
    <main class="main-wrap">
        <%- include('../partials/ecomAdminHeader') %>
        <section class="content-main">
            
            <div class="row">
                
                <div class="col">
                    <div class="content-header d-flex">
                        <h2 class="content-title">Product Details - <%= products.name %></h2>
                        <div>
                            <a href="/admin/edit-product?id=<%= products._id.toString() %>" class="btn btn-md rounded font-sm hover-up" style="width: 120px;">Edit Product</a>
                        </div>
                    </div>
                </div>
                <div class="card mb-4">
                    <div class="card-header">
                        <h5>Media</h5>
                    </div>
                    <div class="card-body d-flex">
                        
    
                        <div class="m-4">
                            <label for="image1">Image 1:</label>
                            <img id="imgView<%= 0 %>" src="public/assets/images/productImages/<%= products.productImages[0] %>" alt=""><br>
                        
                        </div>
                        <br>
                        <div class="m-4 ">
                        <label for="image2">Image 2:</label>
                        <img id="imgView<%= 1 %>" src="public/assets/images/productImages/<%= products.productImages[1] %>" alt="">

                    </div>
                        <br>
                        <div class="m-4 ">
                        <label for="image3">Image 3:</label>
                        <img id="imgView<%= 3 %>" src="public/assets/images/productImages/<%= products.productImages[2] %>" alt="">
                    </div>
                        <br>
                        <div class="m-4 ">
                        <label for="image4">Image 4:</label>
                        <img id="imgView<%= 3 %>" src="public/assets/images/productImages/<%= products.productImages[3] %>" alt="">
                    </div>
                        <br>

                        

                        
                
                    </div>
                    
                    
                </div>
        
            
                <div class="col-lg-6">
                    <div class="card mb-4">
                        <div class="card-body">
                            <h5 class="my-2">Product Info</h5>
                            <div class="mb-4">
                                <h6 class="mt-3">Product title</h6>
                                <p><%= products.name %></p>
                            </div>
            
                            <div class="row gx-3">
                                <div class="mb-3">
                                    <h6 class="mt-3">Quantity</h6>
                                    <p><%= products.quantity %></p>
                                </div>
                            </div>
            
                            <div class="mb-2">
                                <h6 class="mt-3">Product Color</h6>
                                <p><%= products.productColor %></p>
                            </div>
                        </div>
                    </div> <!-- card end// -->
                    <div class="card mb-4">
                        <div class="card-body">
                            <h5 class="my-2">Product Specifications</h5>
            
                            <div class="row gx-3">
                                <div class="mb-3 col-lg-6">
                                    <h6 class="mt-3">Battery</h6>
                                    <p><%= products.battery %></p>
                                </div>
            
                                <div class="mb-3 col-lg-6">
                                    <h6 class="mt-3">Ram</h6>
                                    <p><%= products.ram %></p>
                                </div>
                            </div>
            
                            <div class="row gx-3">
                                <div class="mb-3 col-lg-6">
                                    <h6 class="mt-3">Rom</h6>
                                    <p><%= products.rom %></p>
                                </div>
            
                                <div class="mb-3 col-lg-6">
                                    <h6 class="mt-3">Expandable</h6>
                                    <p><%= products.expandable %></p>
                                </div>
                            </div>
            
                            <div class="row gx-3">
                                <div class="mb-3 col-lg-6">
                                    <h6 class="mt-3">RearCam</h6>
                                    <p><%= products.rearCam %></p>
                                </div>
            
                                <div class="mb-3 col-lg-6">
                                    <h6 class="mt-3">FrontCam</h6>
                                    <p><%= products.frontCam %></p>
                                </div>
                            </div>
            
                            <div class="mb-4">
                                <h6 class="mt-3">Processor</h6>
                                <p><%= products.processor %></p>
                            </div>
                        </div>
                    </div> <!-- card end// -->
                </div>
            
                <div class="col-lg-6">
                    <div class="card mb-4"> 
                        <div class="card-body mb-4">
                            <div class="mb-4">
                                <h6 class="mt-3">Price</h6>
                                <p><%= products.price %></p>
                            </div>
                            <div class="mb-4">
                                <h6 class="mt-3">Discount Price</h6>
                                <p><%= products.discountPrice %></p>
                            </div>
            
                            <hr>
                            <div class="mb-4">
                                <h6 class="mt-3">Category</h6>
                                <p><%= products.category %></p>
                            </div>
                        </div>
                    </div> <!-- card end// -->
                </div>
            </div>
            
            
        </section>
        <footer class="main-footer font-xs">
            <div class="row pb-30 pt-15">
                <div class="col-sm-6">
                    <script>
                    document.write(new Date().getFullYear())
                    </script> Â©, Evara - HTML Ecommerce Template .
                </div>
                <div class="col-sm-6">
                    <div class="text-sm-end">
                        All rights reserved
                    </div>
                </div>
            </div>
        </footer>
    </main>
    <script src="assetsB/js/vendors/jquery-3.6.0.min.js"></script>
    <script src="assetsB/js/vendors/bootstrap.bundle.min.js"></script>
    <script src="assetsB/js/vendors/select2.min.js"></script>
    <script src="assetsB/js/vendors/perfect-scrollbar.js"></script>
    <script src="assetsB/js/vendors/jquery.fullscreen.min.js"></script>
    <script src="assetsB/js/vendors/chart.js"></script>
    <!-- Main Script -->
    <script src="assetsB/js/main.js" type="text/javascript"></script>
    <script src="assetsB/js/custom-chart.js" type="text/javascript"></script>
</body>


