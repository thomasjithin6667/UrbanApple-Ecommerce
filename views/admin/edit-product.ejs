<head>
    <meta charset="utf-8">
    <title>Admin Dashboard</title>
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta property="og:title" content="">
    <meta property="og:type" content="">
    <meta property="og:url" content="">
    <meta property="og:image" content="">
    <!-- Favicon -->
    <link rel="shortcut icon" href="assets/images/icons/apple-touch-icon.png" type="image/x-icon">
  <link rel="apple-touch-icon" sizes="180x180" href="assets/images/icons/apple-touch-icon.png">
  <link rel="icon" type="image/png" sizes="32x32" href="assets/images/icons/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="16x16" href="assets/images/icons/favicon-16x16.png">
    <!-- Template CSS -->
    <link href="assetsB/css/main.css" rel="stylesheet" type="text/css" />
</head>

<body>
    <div class="screen-overlay"></div>
    <%- include('../partials/ecomAdminAside') %>
   
    <main class="main-wrap">
        <%- include('../partials/ecomAdminHeader') %>
        <section class="content-main">
            <form form action="" method="POST" enctype="multipart/form-data">
                <input type="hidden" name="productId" value="<%=products._id%>"  required>
            <div class="row">
                <div class="col-9">
                    <div class="content-header">
                        <h2 class="content-title"> Edit Product- <%= products.name %></h2>
                        
                    </div>
                </div>
                <% if (typeof message !== 'undefined' && message) { %>
                    <p style="color: rgb(0, 125, 209);" id="message"><%= message %></p>
                <% } %>
             
                <div class="col-lg-6">
                    <div class="card mb-4">
                        <div class="card-body">
                            <h5 class="my-2">Product Info</h5>
                            <div class="mb-4">
                                <label for="product_title" class="form-label">Product title</label>
                                <input type="text" placeholder="Type here" class="form-control" name="name"  value="<%= products.name %>" r>
                            </div>
                            <div class="row gx-3">
                               
                                <div class="  mb-3">
                                    <label for="product_size" class="form-label">Quanity</label>
                                    <input type="number" placeholder="Type here" class="form-control" name="quantity"  value="<%= products.quantity %>" >
                                </div>
                            </div>
                          
                            <div class="mb-4">
                                <label for="product_brand" class="form-label">product Color</label>
                                <input type="text" placeholder="Type here" class="form-control" name="productColor"  value="<%= products.productColor%>" >
                            </div>
                        </div>
                    </div> <!-- card end// -->
                    <div class="card mb-4">
                        <div class="card-body">
                            <h5 class="my-2">Product Specifications</h5>
                            
                            <div class="row gx-3 ">
                                <div class="  mb-3  col-lg-6">
                                    <label for="product_title" class="form-label">Battery</label>
                                    <input type="text" placeholder="Type here" class="form-control" name="battery" value="<%= products.battery %>" >
                                </div>
                               
                                <div class="  mb-3  col-lg-6">
                                    <label for="product_size" class="form-label">Ram</label>
                                    <input type="text" placeholder="Type here" class="form-control" name="ram" value="" >
                                </div>
                            </div>
                             
                            <div class="row gx-3 ">
                                <div class="  mb-3  col-lg-6">
                                    <label for="product_title" class="form-label">Rom</label>
                                    <input type="text" placeholder="Type here" class="form-control" name="rom" value="" >
                                </div>
                               
                                <div class="  mb-3  col-lg-6">
                                    <label for="product_size" class="form-label">Expandable</label>
                                    <input type="text" placeholder="Type here" class="form-control" name="expandable" value="" >
                                </div>
                            </div>
                             
                            <div class="row gx-3 ">
                                <div class="  mb-3  col-lg-6">
                                    <label for="product_title" class="form-label">RearCam</label>
                                    <input type="text" placeholder="Type here" class="form-control" name="rearCam" value="<%= products.rearCam %>" >
                                </div>
                               
                                <div class="  mb-3  col-lg-6">
                                    <label for="product_size" class="form-label">FrontCam</label>
                                    <input type="text" placeholder="Type here" class="form-control" name="frontCam" value="<%= products.frontCam %>" >
                                </div>
                            </div>
                          
                            <div class="mb-4">
                                <label for="product_brand" class="form-label">Processor</label>
                                <input type="text" placeholder="Type here" class="form-control" name="processor" value="<%= products.processor %>" >
                            </div>
                        </div>
                    </div> <!-- card end// -->
                    <div class="card mb-4">
                        <div class="card-body">
                            <div>
                                <label class="form-label">Images</label>
                                <label for="productImages">Product Images (up to 4 images):</label>
                                <input type="file" id="productImages" name="productImages" multiple accept="image/jpeg, image/png" >
                                <br>
                            </div>
                        </div>
                    </div> <!-- card end// -->
                </div>
            
                <div class="col-lg-3">
                    <div class="card mb-4">
                        <div class="card-body">
                            <div class="mb-4">
                                <label class="form-label">Price</label>
                                <input type="number" placeholder="Type here" class="form-control" name="price" value="<%= products.price %>" >
                            </div>
                            <div class="mb-4">
                                <label class="form-label">Discount Price</label>
                                <input type="number" placeholder="Type here" class="form-control" name="discountPrice" value="<%= products.discountPrice %>" >
                            </div>
                          
              
                            <hr>
                            <h5 class="mb-3">Categories</h5>
                            <% if (category && category.length > 0) { %>
                                <div class="form-group">
                                  <% for (let i = 0; i < category.length; i++) { %>
                                    <div class="form-check">
                                      <input class="form-check-input" name="category" type="radio" value="<%= category[i].category %>" id="category<%= i %>"
                                        <% if (category[i].category === products.category) { %>checked<% } %>>
                                      <label class="form-check-label" for="category<%= i %>"><%= category[i].category %></label>
                                    </div>
                                  <% } %>
                                </div>
                              <% } %>
                    
                         
                        </div>
                    </div> <!-- card end// -->
                </div>
            </div>
            <div>
                           
                <button type="submit" class="btn btn-md rounded font-sm hover-up">Apply changes</button>
            </div>
        </form>
        </section>
        <footer class="main-footer font-xs">
            <div class="row pb-30 pt-15">
                <div class="col-sm-6">
                    <script>
                    document.write(new Date().getFullYear())
                    </script> Â©, Evara - HTML Ecommerce Template .
                </div>
                <div class="col-sm-6">
                    <div class="text-sm-end">
                        All rights reserved
                    </div>
                </div>
            </div>
        </footer>
    </main>
    <script src="assetsB/js/vendors/jquery-3.6.0.min.js"></script>
    <script src="assetsB/js/vendors/bootstrap.bundle.min.js"></script>
    <script src="assetsB/js/vendors/select2.min.js"></script>
    <script src="assetsB/js/vendors/perfect-scrollbar.js"></script>
    <script src="assetsB/js/vendors/jquery.fullscreen.min.js"></script>
    <script src="assetsB/js/vendors/chart.js"></script>
    <!-- Main Script -->
    <script src="assetsB/js/main.js" type="text/javascript"></script>
    <script src="assetsB/js/custom-chart.js" type="text/javascript"></script>
</body>


