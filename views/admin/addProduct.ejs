        <section class="content-main">
            <form form action="" method="POST" enctype="multipart/form-data">
            <div class="row">
                <div class="col-9">
                    <div class="content-header">
                        <h2 class="content-title">Add New Product</h2>
                        <div>
                           
                            <button type="submit" class="btn btn-md rounded font-sm hover-up">Create Product</button>
                        </div>
                    </div>
                </div>
                <% if (typeof message !== 'undefined' && message) { %>
                    <p style="color: rgb(0, 125, 209);" id="message"><%= message %></p>
                <% } %>
             
                <div class="col-lg-6">
                    <div class="card mb-4">
                        <div class="card-body">
                            <h5 class="my-2">Product Info</h5>
                            <div class="mb-4">
                                <label for="product_title" class="form-label">Product title</label>
                                <input type="text" placeholder="Type here" class="form-control" name="name" required>
                            </div>
                            <div class="row gx-3">
                               
                                <div class="  mb-3">
                                    <label for="product_size" class="form-label">Quanity</label>
                                    <input type="number" placeholder="Type here" class="form-control" name="quantity">
                                </div>
                            </div>
                          
                            <div class="mb-4">
                                <label for="product_brand" class="form-label">product Color</label>
                                <input type="text" placeholder="Type here" class="form-control" name="productColor">
                            </div>
                        </div>
                    </div> <!-- card end// -->
                    <div class="card mb-4">
                        <div class="card-body">
                            <h5 class="my-2">Product Specifications</h5>
                            
                            <div class="row gx-3 ">
                                <div class="  mb-3  col-lg-6">
                                    <label for="product_title" class="form-label">Battery</label>
                                    <input type="text" placeholder="Type here" class="form-control" name="battery">
                                </div>
                               
                                <div class="  mb-3  col-lg-6">
                                    <label for="product_size" class="form-label">Ram</label>
                                    <input type="text" placeholder="Type here" class="form-control" name="ram">
                                </div>
                            </div>
                             
                            <div class="row gx-3 ">
                                <div class="  mb-3  col-lg-6">
                                    <label for="product_title" class="form-label">Rom</label>
                                    <input type="text" placeholder="Type here" class="form-control" name="rom">
                                </div>
                               
                                <div class="  mb-3  col-lg-6">
                                    <label for="product_size" class="form-label">Expandable</label>
                                    <input type="text" placeholder="Type here" class="form-control" name="expandable">
                                </div>
                            </div>
                             
                            <div class="row gx-3 ">
                                <div class="  mb-3  col-lg-6">
                                    <label for="product_title" class="form-label">RearCam</label>
                                    <input type="text" placeholder="Type here" class="form-control" name="rearCam">
                                </div>
                               
                                <div class="  mb-3  col-lg-6">
                                    <label for="product_size" class="form-label">FrontCam</label>
                                    <input type="text" placeholder="Type here" class="form-control" name="frontCam">
                                </div>
                            </div>
                          
                            <div class="mb-4">
                                <label for="product_brand" class="form-label">Processor</label>
                                <input type="text" placeholder="Type here" class="form-control" name="processor">
                            </div>
                        </div>
                    </div> <!-- card end// -->
                    <div class="card mb-4">
                        <div class="card-body">
                            <div>
                                <label class="form-label">Images</label>
                                <label for="productImages">Product Images (up to 4 images):</label>
                                <input type="file" id="productImages" name="productImages" multiple accept="image/jpeg, image/png" >
                                <br>
                            </div>
                        </div>
                    </div> <!-- card end// -->
                </div>
            
                <div class="col-lg-3">
                    <div class="card mb-4">
                        <div class="card-body">
                            <div class="mb-4">
                                <label class="form-label">Price</label>
                                <input type="number" placeholder="Type here" class="form-control" name="price">
                            </div>
                            <div class="mb-4">
                                <label class="form-label">Discount Price</label>
                                <input type="number" placeholder="Type here" class="form-control" name="discountPrice">
                            </div>
                          
              
                            <hr>
                            <h5 class="mb-3">Categories</h5>
                            <div class="form-check">
                                <input class="form-check-input" type="radio" value="iphone" id="product-cat" name="category">
                                <label class="form-check-label" for="product-cat">iphone</label>
                            </div>
                            
                            <div class="form-check">
                                <input class="form-check-input" type="radio" value="ipad" id="product-cat-1" name="category">
