                <div class="card-body">
                    <div class="table">
                        <div class="table">

                            <% if (users.length> 0) { %>
                                <table id="users"  class="table">
                                  <thead class="bg-light">
                                    <tr>
                                      <th>Name</th>
                                      <th>Email</th>
                                      <th>Mobile</th>
                                      <th style="width: 130px;">Status</th>
                                      <th style="width: 130px;">Actions</th>
                                    </tr>
                                  </thead>
                                  <tbody>
                                    <% for (let i=0; i < users.length; i++) { %>
                                      <tr>
                                        <td>
                                          <div class="d-flex align-items-center">
                                            <img src="assets/images/userImages/<%= users[i].image %>" alt="" style="width: 45px; height: 45px"
                                              class="rounded-circle" />
                                            <div class="ms-3">
                                              <p class="fw-bold mb-1">
                                                <%= users[i].name %>
                                              </p>
                                            </div>
                                          </div>
                                        </td>
                                        <td>
                                          <p class="fw-normal mb-1">
                                            <%= users[i].email %>
                                          </p>
                                        </td>
                                        <td>
                                          <p class="fw-normal mb-1">
                                            <%= users[i].mobile %>
                                          </p>
                                        </td>
                                        <td>
                                            <p class="fw-normal mb-1">
                                                <% if (users[i].isBlocked ) { %><span class="badge badge-pill badge-soft-danger">Blocked</span><% } else { %><span class="badge badge-pill badge-soft-primary">Unblocked</span>
                                                <% } %>
                                            
                                            </p>
                                          </td>
                          
                                        <td>
                                            <% if (users[i].isBlocked) { %>
                                                <a href="/admin/block-user?id=<%= users[i]._id.toString() %>">
                                                  <button type="button" class="btn btn-outline-danger ms-1">
                                                    Unblock
                                                  </button>
                                                </a>
                                              <% } else { %>
                                                <a href="/admin/block-user?id=<%= users[i]._id.toString() %>">
                                                  <button type="button" class="btn btn-outline-primary ms-1">
                                                    Block
                                                  </button>
                                                </a>
                                              <% } %>
                                              
                          
                          
                          
                                        </td>
                                      </tr>
                                      <% } %>
                                  </tbody>
                                </table>
                                <% } else { %>
                                  <td>User not found</td>
                                  <% } %>
                        </div>
                    </div> <!-- table-responsive end// -->
                </div>
