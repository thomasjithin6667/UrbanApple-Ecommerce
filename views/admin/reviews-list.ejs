<head>
    <meta charset="utf-8">
    <title>Admin Dashboard</title>
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta property="og:title" content="">
    <meta property="og:type" content="">
    <meta property="og:url" content="">
    <meta property="og:image" content="">
    <!-- Favicon -->
    <link rel="shortcut icon" href="assets/images/icons/apple-touch-icon.png" type="image/x-icon">
    <link rel="apple-touch-icon" sizes="180x180" href="assets/images/icons/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="assets/images/icons/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="assets/images/icons/favicon-16x16.png">
    <!-- Template CSS -->
    <link href="assetsB/css/main.css" rel="stylesheet" type="text/css" />
</head>

<body>
    <div class="screen-overlay"></div>
    <%- include('../partials/ecomAdminAside') %>
    <main class="main-wrap">
        <%- include('../partials/ecomAdminHeader') %>
        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Product Reviews</h2>
                 
                </div>


               


            </div>
            <div class="card">
                <div class="row align-items-center mx-2 my-4">
             
                    
           
                  </div>
                <div class="card-body">
                    <div class="row">

                        <div class="col-md">
                            <div>


                                <table class="table" id="products">
                                    <thead>
                                        <tr>
                                            <th>Product</th>
                                            <th>Reviewer</th>
                                            <th>Date</th>
                                            <th>Title</th>
                                            <th style="width: 400px;">Review</th>
                                            <th style="width: 70px;">Rating</th>
                                            <th class="text-end">Actions</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% userReviews.forEach(review => { %>
                                            <tr>
                                                <td>
                                                    <img src="public/assets/images/productImages/<%= review.productId.productImages[0] %>" alt="Category Image" style="width: 45px;" />
                                                </td>
                                                <td>
                                                    <%= review.userId.name %>
                                                </td>
                                                <td>
                                                    <%= review.date.toDateString()%>
                                                </td>
                                                <td>
                                                    <%= review.title %>
                                                </td>
                                                <td>
                                                    <%= review.description %>
                                                </td>
                                                <td>
                                                   
                                                    <ul class="rating-stars">
                                                        <li style="width: <%= review.starRating * 20 %>%;" class="stars-active">
                                                            <img src="/assetsB/imgs/icons/stars-active.svg" alt="stars" />
                                                        </li>
                                                        <li>
                                                            <img src="/assetsB/imgs/icons/starts-disable.svg" alt="stars" />
                                                        </li>
                                                    </ul>
                                                </td>
                                                <td class="text-end">
                                                    <div class="dropdown">
                                                        <a href="#" data-bs-toggle="dropdown" class="btn btn-light rounded btn-sm font-sm">
                                                            <i class="material-icons md-more_horiz"></i>
                                                        </a>
                                                        <div class="dropdown-menu">
                                                            <a class="dropdown-item"  href="/admin/reviews-delete?reviewId=<%=review._id%>">Delete Review</a>
                                                        </div>
                                                    </div>
                                                </td>
                                            </tr>
                                        <% }) %>
                                    </tbody>
                                </table>
                                
                                  

                            </div>
                        </div> <!-- .col// -->
                    </div> <!-- .row // -->
                </div> <!-- card body .// -->
            </div> <!-- card .// -->

              <div class="pagination-area mt-30 mb-50">
                <nav aria-label="Page navigation">
                  <ul class="pagination justify-content-start">
                    <% if (currentPage > 1) { %>
                      <li class="page-item">
                        <a class="page-link" href="?page=<%= currentPage - 1 %>">
                          <i class="material-icons md-chevron_left"></i>
                        </a>
                      </li>
                    <% } %>
                
                    <% for (let i = 1; i <= totalPages; i++) { %>
                      <li class="page-item <% if (i === currentPage) { %>active<% } %>">
                        <a class="page-link" href="?page=<%= i %>"><%= i %></a>
                      </li>
                    <% } %>
                
                    <% if (currentPage < totalPages) { %>
                      <li class="page-item">
                        <a class="page-link" href="?page=<%= currentPage + 1 %>">
                          <i class="material-icons md-chevron_right"></i>
                        </a>
                      </li>
                    <% } %>
                  </ul>
                </nav>
              </div>
              
        </section> <!-- content-main end// -->

        <footer class="main-footer font-xs">
            <div class="row pb-30 pt-15">
                <div class="col-sm-6">
                    <script>
                        document.write(new Date().getFullYear())
                    </script> Â©, UrbanApple.com 
                </div>
                <div class="col-sm-6">
                    <div class="text-sm-end">
                        All rights reserved
                    </div>
                </div>
            </div>
        </footer>
    </main>



        
        
        

    <script src="assetsB/js/vendors/jquery-3.6.0.min.js"></script>
    <script src="assetsB/js/vendors/bootstrap.bundle.min.js"></script>
    <script src="assetsB/js/vendors/select2.min.js"></script>
    <script src="assetsB/js/vendors/perfect-scrollbar.js"></script>
    <script src="assetsB/js/vendors/jquery.fullscreen.min.js"></script>
    <script src="assetsB/js/vendors/chart.js"></script>
    
    <!-- Main Script -->
    <script src="assetsB/js/main.js" type="text/javascript"></script>
    <script src="assetsB/js/custom-chart.js" type="text/javascript"></script>
</body>


