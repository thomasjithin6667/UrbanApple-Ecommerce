<head>
    <meta charset="utf-8">
    <title>Admin Dashboard</title>
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta property="og:title" content="">
    <meta property="og:type" content="">
    <meta property="og:url" content="">
    <meta property="og:image" content="">
    <!-- Favicon -->
    <link rel="shortcut icon" href="/assets/images/icons/apple-touch-icon.png" type="image/x-icon">
    <link rel="apple-touch-icon" sizes="180x180" href="/assets/images/icons/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/assets/images/icons/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/assets/images/icons/favicon-16x16.png">
    <!-- Template CSS -->
    <link href="/assetsB/css/main.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css" integrity="sha512-z3gLpd7yknf1YoNbCzqRKc4qyor8gaKU1qmn+CShxbuBusANI9QpRohGBreCFkKxLhei6S9CQXFEbbKuqLg0DA==" crossorigin="anonymous" referrerpolicy="no-referrer" />
</head>

<body>
    <div class="screen-overlay"></div>
    <%- include('../partials/ecomAdminAside') %>
    <main class="main-wrap">
        <%- include('../partials/ecomAdminHeader') %>
        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Coupon Details</h2>
                    <p>Details for Coupon ID: <%= coupon._id %> </p>
                </div>
            </div>
            <div class="card">
                <header class="card-header">
                    <div class="row align-items-center">
                        <div class="col-lg-6 col-md-6 mb-lg-0 mb-15">
                        
                          
                            <h4> <span class="text-muted"> Coupon Code</span> : <%= coupon.code %></h4> <br>

                            <small class="text-muted"> Coupon Satus :</small> <b> 
                                <% if (coupon.isListed) { %>
                                    <span class="badge badge-pill badge-soft-primary">Listed</span>
                                  <% } else { %>
                                    <span class="badge badge-pill badge-soft-danger">Unlisted</span>
                                  <% } %>
                            </b>
                         
                           
                        </div>
                        <div class="col-lg-6 col-md-6 ms-auto text-md-end">
                            <span>
                                <i class="material-icons md-calendar_today"></i><small class="text-muted"> Created Date :</small> <b><%= coupon.createdDate.toDateString() %> </b>
                            </span> <br><br>
                         
                              <small class="text-muted"> Expiry Date :</small> <b><%= coupon.expiry.toDateString() %> </b>
                            </span> 
                    
                            


                        </div>
                    </div>
                </header> <!-- card-header end// -->
                <div class="card-body">
                    <div class="row mb-50 mt-20 order-info-wrap text-center">
                        <div class="col-md-4">
                            <article class="icontext align-items-start">
                                <span class="icon icon-sm rounded-circle bg-primary-light">
                                    <i class="fa-solid fa-tag"></i>
                                </span>
                                <div class="text">
                                    <h6 class="mb-1">Coupon Type</h6>
                                    <p class="mb-1"><%= coupon.type %>
                                    </p>
                                  
                                </div>
                            </article>
                        </div> <!-- col// -->
                        <div class="col-md-4">



                            <% if (coupon.type === 'fixed') { %>
                                <article class="icontext align-items-start">
                                    <span class="icon icon-sm rounded-circle bg-primary-light">
                                        <i class="fa-solid fa-money-check-dollar"></i>
                                    </span>
                                    <div class="text">
                                        <h6 class="mb-1"> Coupon Discount</h6>
                                        <p class="mb-1">â‚¹<%= coupon.discount %>
                                        </p>
                                      
                                    </div>
                                </article>
                                
                              <% } else { %>
                                <article class="icontext align-items-start">
                                    <span class="icon icon-sm rounded-circle bg-primary-light">
                                        <i class="fa-solid fa-percent"></i>
                                    </span>
                                    <div class="text">
                                        <h6 class="mb-1"> Coupon Discount</h6>
                                        <p class="mb-1"><%= coupon.discount %> %
                                        </p>
                                      
                                    </div>
                                </article>
                              <% } %>
                          
                        </div> <!-- col// -->
                        <div class="col-md-4">
                            <article class="icontext align-items-start">
                                <span class="icon icon-sm rounded-circle bg-primary-light">
                                    <i class="fa-solid fa-user"></i>
                                </span>
                                <div class="text">
                                    <h6 class="mb-1">Coupon Limit</h6>
                                    <p class="mb-1"> <%= coupon.limit %>
                                    </p>
                                    
                                </div>
                                
                            </article>
                        </div> <!-- col// -->
                        <div class="col-md-4 mt-50">
                            <article class="icontext align-items-start">
                                <span class="icon icon-sm rounded-circle bg-primary-light">
                                    <i class="fa-solid fa-cart-shopping"></i>
                                </span>
                                <div class="text">
                                    <h6 class="mb-1">Min Cart Amount</h6>
                                    <p class="mb-1"> <%= coupon.minCartAmt %>
                                    </p>
                                    
                                </div>
                                
                            </article>
                        </div> <!-- col// -->
                        <div class="col-md-4 mt-50" >
                            <article class="icontext align-items-start">
                                <span class="icon icon-sm rounded-circle bg-primary-light">
                                    <i class="fa-brands fa-buffer"></i>
                                </span>
                                <div class="text">
                                    <h6 class="mb-1">Max Amount</h6>
                                    <p class="mb-1"> <%= coupon.maxRedeemableAmt %>
                                    </p>
                                    
                                </div>
                                
                            </article>
                        </div> <!-- col// -->
                        <div class="col-md-4 mt-50">
                            <article class="icontext align-items-start">
                                <span class="icon icon-sm rounded-circle bg-primary-light">
                                    <i class="fa-solid fa-chart-line"></i>
                                </span>
                                <div class="text">
                                    <h6 class="mb-1">Active Status</h6>
                                    <p class="mb-1"> <%= coupon.isListed %>
                                    </p>
                                    
                                </div>
                                
                            </article>
                        </div> <!-- col// -->
                    </div> <!-- row // -->
                    <div class="row">
                        <div class="col-lg">
                            <h4 class="mb-4">User Details for Coupon Usage</h4>
                            <div class="table-responsive">
                                <table class="table ">
                                    <thead>
                                        <tr>
                                            <th>User Name</th>
                                            <th>Email</th>
                                            <th>Coupon Code</th>
                                            <th>Discount</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <!-- Replace the following rows with actual user data -->
                                        <% users.forEach(user =>{ %>
                                        <tr>
                                            <td><%= user.name %></td>
                                            <td><%= user.email %></td>
                                            <td><%= coupon.code %></td>
                                            <td><%= coupon.discount %></td>
                                        </tr>
                                        <% }) %>
                                        <!-- Add more rows as needed -->
                                    </tbody>
                                </table>
                            </div>
                        </div> <!-- col// -->
                        
                    
                    </div>
                </div> <!-- card-body end// -->
            </div> <!-- card end// -->
        </section>
        <footer class="main-footer font-xs">
            <div class="row pb-30 pt-15">
                <div class="col-sm-6">
                    <script>
                        document.write(new Date().getFullYear())
                    </script> Â©, Evara - HTML Ecommerce Template .
                </div>
                <div class="col-sm-6">
                    <div class="text-sm-end">
                        All rights reserved
                    </div>
                </div>
            </div>
        </footer>
    </main>
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
                   <script src="https://cdn.datatables.net/1.13.6/js/jquery.dataTables.min.js"></script>
                      <script src="https://cdn.datatables.net/1.13.6/js/dataTables.bootstrap5.min.js"></script>
                    <script>
                      $(document).ready(function () {
                   new DataTable('#products');

                    })
                     </script>
    <script src="/assetsB/js/vendors/jquery-3.6.0.min.js"></script>
    <script src="/assetsB/js/vendors/bootstrap.bundle.min.js"></script>
    <script src="/assetsB/js/vendors/select2.min.js"></script>
    <script src="/assetsB/js/vendors/perfect-scrollbar.js"></script>
    <script src="/assetsB/js/vendors/jquery.fullscreen.min.js"></script>
    <script src="/assetsB/js/vendors/chart.js"></script>
    
    <!-- Main Script -->
    <script src="/assetsB/js/main.js" type="text/javascript"></script>
    <script src="/assetsB/js/custom-chart.js" type="text/javascript"></script>
</body>


